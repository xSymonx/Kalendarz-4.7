<UserControl x:Class="Kalendarz.Blank"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Kalendarz"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100">

    <!--Pusta kontrolka wykorzysytwana w algorytmie wyświetalania dni-->
    <Grid>
        <TextBox HorizontalAlignment="Center" 
                 Margin="0,32,0,0" TextWrapping="Wrap" 
                 VerticalAlignment="Top" Width="100" 
                 Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" SelectionBrush="{x:Null}"
                 Visibility="Hidden"  />
    </Grid>
</UserControl>
